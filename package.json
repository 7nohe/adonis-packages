{
  "name": "adonis-packages",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --watch",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "quick:test": "node ace test",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js",
    "#types/*": "./types/*.js"
  },
  "dependencies": {
    "@adonisjs/cache": "1.0.0-0",
    "@adonisjs/core": "^6.1.5-31",
    "@adonisjs/inertia": "1.0.0-1",
    "@adonisjs/lucid": "19.0.0-3",
    "@adonisjs/session": "^7.0.0-13",
    "@adonisjs/shield": "^8.0.0-8",
    "@adonisjs/static": "^1.0.3-2",
    "@headlessui/vue": "^1.7.16",
    "@inertiajs/vue3": "^1.0.14",
    "@poppinss/utils": "^6.5.1",
    "@sindresorhus/slugify": "^2.2.1",
    "@vinejs/vine": "^1.6.0",
    "@vueuse/core": "^10.6.1",
    "edge.js": "^6.0.0-10",
    "fuse.js": "^7.0.0",
    "github-markdown-css": "^5.4.0",
    "globby": "^14.0.0",
    "gsap": "^3.12.2",
    "highlight.js": "^11.9.0",
    "js-yaml": "^4.1.0",
    "luxon": "^3.4.3",
    "markdown-it": "^13.0.2",
    "markdown-it-anchor": "^8.6.7",
    "millify": "^6.1.0",
    "p-limit": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^6.1.3-25",
    "@adonisjs/eslint-config": "^1.1.8",
    "@adonisjs/prettier-config": "^1.1.8",
    "@adonisjs/tsconfig": "^1.1.8",
    "@adonisjs/vite": "^1.0.0",
    "@faker-js/faker": "^8.3.1",
    "@iconify/json": "^2.2.141",
    "@iconify/utils": "^2.1.11",
    "@japa/api-client": "^2.0.1",
    "@japa/assert": "^2.0.0",
    "@japa/file-system": "^2.0.1",
    "@japa/plugin-adonisjs": "^2.0.0",
    "@japa/runner": "^3.0.4",
    "@julr/eslint-config-prettier": "^1.0.0",
    "@julr/eslint-config-vue": "^1.0.0",
    "@julr/unocss-preset-forms": "^0.0.6",
    "@swc/core": "1.3.82",
    "@types/js-yaml": "^4.0.9",
    "@types/luxon": "^3.3.3",
    "@types/markdown-it": "^13.0.6",
    "@types/node": "^20.8.9",
    "@unocss/reset": "^0.57.3",
    "@vitejs/plugin-vue": "^4.4.1",
    "eslint": "^8.52.0",
    "eslint-plugin-perfectionist": "^2.3.0",
    "pino-pretty": "^10.2.3",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "unocss": "^0.57.3",
    "unocss-preset-radix": "^2.5.5",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.22"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app",
    "plugins": [
      "perfectionist"
    ],
    "ignorePatterns": [
      "public/**/*"
    ],
    "rules": {
      "@typescript-eslint/consistent-type-imports": [
        "error",
        {
          "prefer": "type-imports",
          "disallowTypeAnnotations": false
        }
      ],
      "perfectionist/sort-imports": [
        "error",
        {
          "type": "line-length",
          "order": "asc",
          "groups": [
            "type",
            "react",
            "nanostores",
            [
              "builtin",
              "external"
            ],
            [
              "internal-type",
              "internal",
              "parent-type",
              "sibling-type",
              "index-type",
              "parent",
              "sibling",
              "index",
              "side-effect",
              "style",
              "object",
              "unknown"
            ]
          ]
        }
      ]
    }
  },
  "prettier": "@adonisjs/prettier-config"
}
